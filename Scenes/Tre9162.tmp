script/source = "extends StaticBody2D

@export var Season = \"Spring\"
@export var Tree_Type = \"Appletree_Default\"

@export var RandomTree_of_Season = true
@export var ExcludeTrees: Array[String]

@onready var Tree_sprite = $Tree_Sprite

var TreeDicionary = {
	1: \"Appletree_Default\",
	2: \"Birchtree_Default\",
	3: \"CherryBlossomtree_Default\",
	4: \"Cursedtree_Default\",
	5: \"DeadOld_Default\",
	6: \"Dragontree_Default\",
	7: \"Orangetree_Default\",
	8: \"Peartree_Default\",
	9: \"Pinetree_Default\",
	10: \"Plumtree_Default\",
}


func _ready():
	if (RandomTree_of_Season): Tree_Type = TreeDicionary[randi_range(1, 10)]
	
	Tree_sprite.play(Tree_Type)
"

