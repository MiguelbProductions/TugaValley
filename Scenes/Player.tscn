script/source = "extends CharacterBody2D

@onready var Player_Anim = $Player_Anim
@onready var Player_RayCast = $Player_RayCast

var ItemSelected_Index = 1
var Slots = [\"Pickaxe\", \"Axe\", \"Hoe\", \"WaterCan\", \"Seed\", null, null, null, null]

const SPEED = 100

var Using_Tool = false

func _input(event):
	if (Input.is_action_pressed(\"Action\")):
		if (Slots[ItemSelected_Index - 1] == \"Pickaxe\" or Slots[ItemSelected_Index - 1] == \"Axe\" or Slots[ItemSelected_Index - 1] == \"Hoe\"): Using_Tool = true
	else:
		if (Using_Tool): Using_Tool = false
	
	if event is InputEventKey:
		if event.keycode >= KEY_1 and event.keycode <= KEY_9:
			var number_pressed = event.keycode - KEY_1 + 1
			
			ItemSelected_Index = number_pressed

func Move():
	if (!Using_Tool):
		var directionX = Input.get_axis(\"Left\", \"Right\")
		var directionY = Input.get_axis(\"Up\", \"Down\")
		
		if directionX: velocity.x = directionX
		else: velocity.x = move_toward(velocity.x, 0, SPEED)
		
		if directionY: velocity.y = directionY
		else: velocity.y = move_toward(velocity.y, 0, SPEED)
		
		velocity = velocity.normalized() * SPEED
		
		move_and_slide()

var LastDirection = Vector2.ZERO

func Animate():
	if (Input.is_action_pressed(\"Action\")):
		if (Slots[ItemSelected_Index - 1] == \"Pickaxe\"):
			if (LastDirection.x == 1): Player_Anim.play(\"Pickaxe_Right\")
			elif (LastDirection.x == -1): Player_Anim.play(\"Pickaxe_Left\")
			elif (LastDirection.y == 1): Player_Anim.play(\"Pickaxe_Down\")
			elif (LastDirection.y == -1): Player_Anim.play(\"Pickaxe_Up\")
		elif (Slots[ItemSelected_Index - 1] == \"Axe\"):
			if (LastDirection.x == 1): Player_Anim.play(\"Axe_Right\")
			elif (LastDirection.x == -1): Player_Anim.play(\"Axe_Left\")
			elif (LastDirection.y == 1): Player_Anim.play(\"Axe_Down\")
			elif (LastDirection.y == -1): Player_Anim.play(\"Axe_Up\")
		elif (Slots[ItemSelected_Index - 1] == \"Hoe\"):
			if (LastDirection.x == 1): Player_Anim.play(\"Hoe_Right\")
			elif (LastDirection.x == -1): Player_Anim.play(\"Hoe_Left\")
			elif (LastDirection.y == 1): Player_Anim.play(\"Hoe_Down\")
			elif (LastDirection.y == -1): Player_Anim.play(\"Hoe_Up\")
		elif (Slots[ItemSelected_Index - 1] == \"WaterCan\"):
			if (LastDirection.x == 1): Player_Anim.play(\"WaterCan_Right\")
			elif (LastDirection.x == -1): Player_Anim.play(\"WaterCan_Left\")
			elif (LastDirection.y == 1): Player_Anim.play(\"WaterCan_Down\")
			elif (LastDirection.y == -1): Player_Anim.play(\"WaterCan_Up\")
	else:
		if (velocity.x > 0): 
			Player_Anim.play(\"Walk_Right\")
			LastDirection = Vector2(1, 0)
		elif (velocity.x < 0): 
			Player_Anim.play(\"Walk_Left\")
			LastDirection = Vector2(-1, 0)
		elif (velocity.y > 0): 
			Player_Anim.play(\"Walk_Down\")
			LastDirection = Vector2(0, 1)
		elif (velocity.y < 0): 
			Player_Anim.play(\"Walk_Up\")
			LastDirection = Vector2(0, -1)
		else: 
			if (LastDirection.x == 1): Player_Anim.play(\"Idle_Right\")
			elif (LastDirection.x == -1): Player_Anim.play(\"Idle_Left\")
			elif (LastDirection.y == 1): Player_Anim.play(\"Idle_Down\")
			elif (LastDirection.y == -1): Player_Anim.play(\"Idle_Up\")

func Focus_RayCast():
	if (LastDirection.x == 1): Player_RayCast.target_position = Vector2(10, 0)
	elif (LastDirection.x == -1): Player_RayCast.target_position = Vector2(-10, 0)
	elif (LastDirection.y == 1): Player_RayCast.target_position = Vector2(0, 10)
	elif (LastDirection.y == -1): Player_RayCast.target_position = Vector2(0, -10)

func _physics_process(delta):
	Move()
	Animate()
	Focus_RayCast()

func AnimationLooped():
	if (\"Pickaxe\" in Player_Anim.animation or \"Axe\" in Player_Anim.animation or \"Hoe\" in Player_Anim.animation):
		if (Player_RayCast.is_colliding()):
			if (Slots[ItemSelected_Index - 1] == \"Pickaxe\"):
				if (\"Stone\" in Player_RayCast.get_collider().get_name()):
					Player_RayCast.get_collider().Break()
			elif (Slots[ItemSelected_Index - 1] == \"Axe\"):
				if (\"Tree\" in Player_RayCast.get_collider().get_name()):
					Player_RayCast.get_collider().Break()
					
		if (Slots[ItemSelected_Index - 1] == \"Hoe\"):
			var Floor = get_tree().root.get_node(\"TestMap/Floor\")
			var PlowedFields = get_tree().root.get_node(\"TestMap/PlowedFields\")
			
			var TileCords_Floor = Floor.local_to_map(Vector2(position.x, position.y))
			var TileCords_PlowedFields = PlowedFields.local_to_map(Vector2(position.x, position.y))
			
			var Tile_FloorAtlasPos = Floor.get_cell_atlas_coords(0, TileCords_Floor)
			var Tile_PlowedFieldsAtlasPos = PlowedFields.get_cell_atlas_coords(0, TileCords_PlowedFields)
			
			if (Tile_FloorAtlasPos == Vector2i(1, 1) and Tile_PlowedFieldsAtlasPos == Vector2i(-1, -1)): 
				PlowedFields.set_cell(0, TileCords_PlowedFields, 0, Vector2i(randi_range(0, 3),0)) 
		elif (Slots[ItemSelected_Index - 1] == \"WaterCan\"):
			var PlowedFields = get_tree().root.get_node(\"TestMap/PlowedFields\")
			var TileCords_PlowedFields = PlowedFields.local_to_map(Vector2(position.x, position.y))
			var Tile_PlowedFieldsAtlasPos = PlowedFields.get_cell_atlas_coords(0, TileCords_PlowedFields)
			
			if (Tile_PlowedFieldsAtlasPos == Vector2i(-1, -1)): 
				PlowedFields.set_cell(0, TileCords_PlowedFields, 0, Vector2i(randi_range(0, 3),0)) 


"

